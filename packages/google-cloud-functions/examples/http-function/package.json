{
  "name": "examples-http-function",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "functions-framework --target=helloWorld",
    "deploy": "gcloud functions deploy helloWorld --runtime nodejs22 --trigger-http",
    "prestart": "npm run build",
    "gcp-build": "npm run build"
  },
  "engines": {
    "node": "24"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/functions-framework": "catalog:prod",
    "@trpc/server": "catalog:prod",
    "trpc-google-cloud-functions": "workspace:*",
    "zod": "catalog:utils"
  },
  "devDependencies": {
    "@trpc-adapters/eslint-config": "workspace:*",
    "@trpc-adapters/typescript-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "catalog:types",
    "@typescript-eslint/parser": "catalog:types",
    "eslint": "catalog:lint",
    "eslint-config-google": "catalog:lint",
    "eslint-plugin-import": "catalog:lint",
    "firebase-functions": "catalog:dev",
    "typescript": "catalog:tsc"
  },
  "private": true
}
