{
  "name": "examples-http-function",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "functions-framework --target=helloWorld",
